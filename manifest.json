{
  "manifest_version": 2,

  "name": "Robinhood to Yahoo Finance",
  "description": "From a stock page on robinhood.com, open the same ticker on Yahoo! Finance.",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://robinhood.com/stocks/*"
      ],
      "js": ["jquery-3.5.1.min.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
   "default_icon": "yahoo-icon.png"
  },
  "permissions": [
   "activeTab",
   "tabs"
  ]
}